<h1>Level</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Block</th>
            <td>{{ entity.block }}</td>
        </tr>
        <tr>
            <th>Time1</th>
            <td>{{ entity.time1 }}</td>
        </tr>
        <tr>
            <th>Time2</th>
            <td>{{ entity.time2 }}</td>
        </tr>
        <tr>
            <th>Time3</th>
            <td>{{ entity.time3 }}</td>
        </tr>
        <tr>
            <th>Timeout</th>
            <td>{{ entity.timeout }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ entity.title }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('level') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('level_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('level_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
